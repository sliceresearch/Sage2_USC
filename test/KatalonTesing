import com.kms.katalon.core.webservice.keyword.WSBuiltInKeywords as WS
import com.kms.katalon.core.webui.keyword.WebUiBuiltInKeywords as WebUiBuiltInKeywords
import com.kms.katalon.core.webui.keyword.WebUiBuiltInKeywords as WebUI
import internal.GlobalVariable as GlobalVariable
import org.openqa.selenium.Keys as Keys

WebUI.openBrowser('')

WebUI.navigateToUrl('https://localhost:9090/index.html')

WebUI.click(findTestObject('Page_SAGE2 - localhost/button_Log in as guest'))

WebUI.waitForPageLoad(5)

TrackPadDisplay = WebUI.getCSSValue(findTestObject('Page_SAGE2 - localhost/div_sage2MobileContainer'), 'Display')

WebUI.navigateToUrl('https://localhost:9090/index.html?teacher=yes')

WebUI.click(findTestObject('Page_SAGE2 - localhost (1)/button_Log in as guest'))

WebUI.waitForPageLoad(5)

SagePointerDisplay = WebUI.getCSSValue(findTestObject('Page_SAGE2 - localhost/div_sage2MobileContainer'), 'Display')

WebUI.closeBrowser()

WebUI.verifyMatch(TrackPadDisplay, 'block', false)

WebUI.verifyMatch(SagePointerDisplay, 'none', false)